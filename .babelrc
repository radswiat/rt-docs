{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src/"],
      "alias": [
        {
          "config": "./config",
          "components": "./components",
          "modules": "./modules",
          "core": "./core"
        }
      ]
    }],
    "transform-runtime"
  ]
}
